{
    "problemMatcher": [
        {
            "owner": "actions-playground-cmake-gcc",
            "pattern": [
                {
                    "regexp": "^(.*):(\\d+):(\\d+):\\s+(?:fatal\\s+)?(warning|error):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            ]
        },
        {
            "owner": "actions-playground-cmake-msvc",
            "pattern": [
                {
                    "regexp": "(.*)\\((\\d+),(\\d+)\\):\\s+(warning|error)\\s+([0-9a-zA-Z]*):\\s+(.+)\\s+\\[(.+)\\]$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "code": 5,
                    "message": 6,
                    "fromPath" : 7
                }
            ]
        }
    ]
}