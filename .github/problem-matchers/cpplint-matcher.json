{
    "problemMatcher": [
        {
            "owner": "actions-playground-cpplint",
            "severity": "error",
            "pattern": [
                {
                "regexp": "^(.+):(\\d+):\\s+(.+)\\[(.+)\\]\\s+\\[(.+)\\]$",
                "file": 1,
                "line": 2,
                "message": 3,
                "code": 4
                }
            ]
        }
    ]
}